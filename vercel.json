{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/data.json",
      "headers": { "Content-Type": "application/json" },
      "dest": "/data.json"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
